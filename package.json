  "author": "Terrence",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^6.12.0",
    "@fastify/view": "^8.2.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "node-cron": "^3.0.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "zod": "^3.22.4",
    "axios": "^1.6.0",
    "nunjucks": "^3.2.4",
    "mysql2": "^3.6.3",
