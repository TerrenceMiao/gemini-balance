  "description": "Gemini API proxy service with load balancing and key management - TypeScript version",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/main.ts",
    "start": "node dist/main.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
